{
    "name": "juzaweb/ticket-support",
    "homepage": "https://juzaweb.com",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Make and managment Ticket support easy for Juzaweb CMS",
    "extra": {
        "juzaweb": {
            "name": "Ticket Support",
            "domain": "jwts",
            "cms_min": "3.3.9",
            "version": "1.0",
            "providers": [
                "Juzaweb\\TicketSupport\\Providers\\TicketSupportServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\TicketSupport\\": "src/"
        }
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
